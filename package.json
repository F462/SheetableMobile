{
	"name": "SheetableMobile",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"lint": "eslint .",
		"start": "react-native start",
		"test": "jest",
		"typecheck": "tsc -p .",
		"verify": "concurrently --names \"typecheck,lint,depcycle,test\" --prefix-colors \"blue,green,yellow,magenta\" --kill-others-on-fail \"yarn typecheck\" \"yarn lint\" \"yarn depcyclecheck\" \"yarn test\"",
		"depcyclecheck": "dpdm index.js --transform --circular --exit-code circular:1 --warning=false --no-tree"
	},
	"dependencies": {
		"@react-native-async-storage/async-storage": "^2.2.0",
		"@react-native-vector-icons/material-design-icons": "^12.4.0",
		"@react-native/new-app-screen": "0.83.1",
		"@reduxjs/toolkit": "^2.11.2",
		"axios": "^1.13.2",
		"i18next": "^25.7.4",
		"react": "19.2.0",
		"react-i18next": "^16.5.3",
		"react-native": "0.83.1",
		"react-native-localize": "^3.6.1",
		"react-native-paper": "^5.14.5",
		"react-native-safe-area-context": "^5.6.2",
		"react-redux": "^9.2.0",
		"redux-persist": "^6.0.0",
		"src": "link:./src"
	},
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@babel/preset-env": "^7.25.3",
		"@babel/runtime": "^7.25.0",
		"@cspell/eslint-plugin": "^9.4.0",
		"@react-native-community/cli": "20.0.0",
		"@react-native-community/cli-platform-android": "20.0.0",
		"@react-native-community/cli-platform-ios": "20.0.0",
		"@react-native/babel-preset": "0.83.1",
		"@react-native/eslint-config": "0.83.1",
		"@react-native/metro-config": "0.83.1",
		"@react-native/typescript-config": "0.83.1",
		"@types/jest": "^29.5.13",
		"@types/react": "^19.2.0",
		"@types/react-test-renderer": "^19.1.0",
		"concurrently": "^9.2.1",
		"dpdm": "^3.14.0",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-ft-flow": "^3.0.11",
		"eslint-plugin-prettier": "^5.5.4",
		"jest": "^29.6.3",
		"prettier": "3.7.4",
		"react-test-renderer": "19.2.0",
		"ts-jest": "^29.4.6",
		"typescript": "^5.8.3"
	},
	"engines": {
		"node": ">=20"
	},
	"packageManager": "yarn@4.12.0",
	"resolutions": {
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-react-native": "5.0.0"
	}
}
